<body id="profile">

  <div id="header">
    <h1> L2T </h1>
    <nav>
      <%= link_to 'SIGN OUT', '/users/sign_out', :method => :delete %>
    </nav>
  </div>

  <div id="info">

    <div id="pic">
      <img id="profile-img" class="vertalign" src="<%=@personal_info[:image_url]%>" />
    </div>

    <div id="about">
      <div id="personal-box">
        <h3><%=@personal_info[:name]%></h3>
        <p><%=@personal_info[:blurb]%></p>
        <div id="personal-button">
          <input type="submit" name="inbox" value="inbox"/>
        </div>
      </div>
    </div>

    <div id="map"> 
      <img src="http://static2.stuff.co.nz/1397882814/958/9959958.jpg">
    </div>
    
  </div>

  <div id="feed">
    <div id="profile-feed">
      <div id="profile-tweets">
        <h2>TWEETS</h2>
        <ul>
          <% @personal_info[:tweets].each do |tweet| %>
            <li><%= tweet.content %></li>
          <% end %>
        </ul> 
      </div>
      <div id="profile-videos">
        <h2>VIDEOS</h2>
        <ul>
          <% @personal_info[:videos].each do |video| %>
            <li><a href="<%=video.url%>"><%= video.title %></a></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>

  <div id="footer"></div>

</body>









